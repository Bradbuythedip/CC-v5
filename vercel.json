{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/images/(.*)",
      "headers": { 
        "cache-control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/assets/images/$1"
    },
    {
      "src": "/assets/(.*)",
      "headers": { 
        "cache-control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|ico|svg|webp)$)",
      "headers": { 
        "cache-control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NPM_FLAGS": "--legacy-peer-deps"
    }
  },
  "env": {
    "VITE_NFT_CONTRACT_ADDRESS": "0x0062481f93e27cdb73ce0fa173c3251dffe40127",
    "VITE_QUAI_RPC_URL": "https://rpc.quai.network",
    "VITE_QUAI_CHAIN_ID": "9000"
  }
}